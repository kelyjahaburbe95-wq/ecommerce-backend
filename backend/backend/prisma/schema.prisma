generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  price       Int
  image       String?
  createdAt   DateTime @default(now())
}
